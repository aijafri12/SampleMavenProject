package tutorial12;

public class SmallestNumber {

	public static void main(String[] args) {

		int x=3;
		boolean prime =true;
		
		for(int i=2;i<x; i++) {
			
			if (x % i==0) {
				
				prime=false;
				
				break;	
			}
			}if (prime==true)
				
				System.out.println( x + "it is a prime number ");
		
			else 
				System.out.println( x + " it is not a prime number");
	
	}

}
