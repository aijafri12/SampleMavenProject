package tutorial12;

public class SmallestNumber {

	public static void main(String[] args) {

		int x[]= {34,333,23,451,231,22,44};
		
		int max=x[0];
		int secmax=x[0];
		
		for (int i=0; i<x.length;i++) {
			
			if (x[i]>max) {
				
				secmax=max;
				max=x[i];
				
			}else if(x[i]>secmax) {
				
				secmax=x[i];
				
			}		
			
			
		}
	System.out.println(max);
		System.out.println(secmax);

		
		
		
	
	}

}
