package tutorial12;

import java.util.Arrays;

public class SmallestNumber {

	public static void main(String[] args) {
	
		String [] x= {"Samad","AJ","Saad","Kawsar","Abdur"};
		
		
		
		String temp;
		
		for(int i=0;i<x.length/2;i++) {
			
			temp=x[i];
			
			x[i]=x[x.length-i-1];
			
			x[x.length-i-1]=temp;
			
		}			System.out.println(Arrays.toString(x));

}


}